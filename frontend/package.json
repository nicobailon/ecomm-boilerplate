{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "check-all": "npm run typecheck && npm run lint && npm run test",
    "check:colors": "node scripts/check-hardcoded-colors.cjs",
    "check:theme": "npm run typecheck && npm run check:colors",
    "test:theme": "vitest run src/__tests__/theme-integration.test.tsx",
    "theme:audit": "grep -r 'bg-\\|text-\\|border-' --include='*.tsx' --exclude-dir=node_modules src/ | grep -E '(gray|emerald|green|red|yellow|blue)-[0-9]' || echo 'No hardcoded colors found!'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:build": "storybook build",
    "storybook:analyze": "storybook build --webpack-stats-json",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "test-storybook --ci"
  },
